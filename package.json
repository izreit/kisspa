{
  "name": "kisspa",
  "version": "0.5.0",
  "description": "lightweight JSX-based library with utility-first CSS to make SPA development simple",
  "type": "module",
  "scripts": {
    "prepare": "npm run build",
    "dev": "vitest",
    "build": "node ./scripts/build.mjs",
    "lint": "biome check",
    "test": "vitest run",
    "cov": "vitest run --coverage",
    "changeset": "changeset",
    "changeset-version": "changeset version",
    "changeset-publish": "changeset publish"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "keywords": [
    "kisspa",
    "ui",
    "reactive",
    "frontend"
  ],
  "author": "izreit",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.11",
    "@types/node": "^22.10.1",
    "@vitest/coverage-istanbul": "^3.0.5",
    "happy-dom": "^15.11.0",
    "terser": "^5.31.1",
    "typescript": "^5.6.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "exports": {
    ".": {
      "types": "./dist/normal/upwind/bundle.d.ts",
      "import": "./dist/normal/upwind/bundle.mjs"
    },
    "./jsx-runtime": {
      "types": "./dist/normal/upwind/bundle.d.ts",
      "import": "./dist/normal/upwind/bundle.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/normal/upwind/bundle.d.ts",
      "import": "./dist/normal/upwind/bundle.mjs"
    },
    "./html": {
      "types": "./dist/normal/html/bundle.d.ts",
      "import": "./dist/normal/html/bundle.mjs"
    },
    "./html/jsx-runtime": {
      "types": "./dist/normal/html/bundle.d.ts",
      "import": "./dist/normal/html/bundle.mjs"
    },
    "./html/jsx-dev-runtime": {
      "types": "./dist/normal/html/bundle.d.ts",
      "import": "./dist/normal/html/bundle.mjs"
    },
    "./reactive": {
      "types": "./dist/normal/reactive/index.d.ts",
      "import": "./dist/normal/reactive/index.mjs"
    },
    "./full": {
      "types": "./dist/full/upwind/bundle.d.ts",
      "import": "./dist/full/upwind/bundle.mjs"
    },
    "./full/jsx-runtime": {
      "types": "./dist/full/upwind/bundle.d.ts",
      "import": "./dist/full/upwind/bundle.mjs"
    },
    "./full/jsx-dev-runtime": {
      "types": "./dist/full/upwind/bundle.d.ts",
      "import": "./dist/full/upwind/bundle.mjs"
    },
    "./html/full": {
      "types": "./dist/full/html/bundle.d.ts",
      "import": "./dist/full/html/bundle.mjs"
    },
    "./html/full/jsx-runtime": {
      "types": "./dist/full/html/bundle.d.ts",
      "import": "./dist/full/html/bundle.mjs"
    },
    "./html/full/jsx-dev-runtime": {
      "types": "./dist/full/html/bundle.d.ts",
      "import": "./dist/full/html/bundle.mjs"
    },
    "./reactive/full": {
      "types": "./dist/full/reactive/index.d.ts",
      "import": "./dist/full/reactive/index.mjs"
    },
    "./extra/cloneutil": {
      "types": "./dist/full/reactive/cloneutil.d.ts",
      "import": "./dist/full/reactive/cloneutil.mjs"
    },
    "./extra/preset/colors": {
      "types": "./dist/full/upwind/preset/colors.d.ts",
      "import": "./dist/full/upwind/preset/colors.mjs"
    },
    "./raw": {
      "types": "./dist/full/upwind/bundle.d.ts",
      "import": "./dist/full/upwind/bundle.raw.mjs"
    },
    "./raw/jsx-dev-runtime": {
      "types": "./dist/full/upwind/bundle.d.ts",
      "import": "./dist/full/upwind/bundle.raw.mjs"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
