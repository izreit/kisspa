{
  "name": "kisspa",
  "version": "0.17.0",
  "description": "lightweight JSX-based library with utility-first CSS to make SPA development simple",
  "type": "module",
  "scripts": {
    "prepare": "npm run build",
    "dev": "vitest",
    "build": "node ./scripts/build.mjs && node ./scripts/stat.mjs",
    "lint": "biome check",
    "test": "tsc -p ./tsconfig.build.json --noEmit && vitest run",
    "cov": "vitest run --coverage",
    "stat": "node ./scripts/build.mjs --stat-only && node ./scripts/stat.mjs --stat-only",
    "changeset": "changeset",
    "changeset-version": "changeset version",
    "changeset-publish": "changeset publish"
  },
  "files": [
    "dist/",
    "dist_supplement/",
    "lib/"
  ],
  "keywords": [
    "kisspa",
    "ui",
    "reactive",
    "frontend"
  ],
  "author": "izreit",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@changesets/cli": "^2.27.11",
    "@types/node": "^25.0.2",
    "@vitest/coverage-istanbul": "^3.0.5",
    "happy-dom": "^20.0.0",
    "terser": "^5.31.1",
    "typescript": "^5.6.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "exports": {
    ".": {
      "types": "./dist/raw/index.d.ts",
      "import": "./dist/bundle/index.mjs"
    },
    "./jsx-runtime": {
      "types": "./dist/raw/html/jsx-runtime.d.ts",
      "import": "./dist/bundle/jsx-runtime.mjs"
    },
    "./h": {
      "types": "./dist/raw/html/h.d.ts",
      "import": "./dist/bundle/h.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/raw/html/jsx-runtime.d.ts",
      "import": "./dist/raw/html/jsx-runtime.js"
    },
    "./html": {
      "types": "./dist/raw/index-html.d.ts",
      "import": "./dist/bundle/index-html.mjs"
    },
    "./html/jsx-runtime": {
      "types": "./dist/raw/html/jsx-runtime.d.ts",
      "import": "./dist/bundle/jsx-runtime.mjs"
    },
    "./html/jsx-dev-runtime": {
      "types": "./dist/raw/html/jsx-runtime.d.ts",
      "import": "./dist/raw/html/jsx-runtime.js"
    },
    "./reactive": {
      "types": "./dist/raw/reactive/index.d.ts",
      "import": "./dist/bundle/index-reactive.mjs"
    },
    "./extra/watch": {
      "types": "./dist/raw/extra/watch/index.d.ts",
      "import": "./dist/bundle/extra/watch/index.mjs"
    },
    "./extra/preset-colors": {
      "types": "./dist/raw/extra/preset-colors/index.d.ts",
      "import": "./dist/bundle/extra/preset-colors/index.mjs"
    },
    "./supplement": "./dist_supplement/supplement/index.js",
    "./supplement/refresh": "./dist_supplement/supplement/refresh/index.js",
    "./supplement/vite-plugin": "./dist_supplement/supplement/vite-plugin/index.js",
    "./raw": "./dist/raw/index.js",
    "./raw/h": "./dist/raw/html/h.mjs",
    "./raw/jsx-runtime": "./dist/raw/html/jsx-runtime.js",
    "./raw/jsx-dev-runtime": "./dist/raw/html/jsx-runtime.js",
    "./raw/extra/watch": "./dist/raw/extra/watch/index.js",
    "./raw/extra/preset-colors": "./dist/raw/extra/preset-colors/index.js",
    "./raw/supplement": "./dist_supplement/supplement/index.js",
    "./raw/supplement/refresh": "./dist_supplement/supplement/refresh/index.js",
    "./raw/supplement/vite-plugin": "./dist_supplement/supplement/vite-plugin/index.js"
  }
}
