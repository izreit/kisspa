    "_build:dist": "vite build && tsc -p ./tsconfig.decl.json"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "keywords": [],
  "author": "izreit",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-istanbul": "^1.6.0",
    "happy-dom": "^14.12.0",
    "tspeg": "^3.3.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./html": "./dist/html/index.js",
    "./reactive": "./dist/reactive/index.js",
    "./jsx-runtime": "./lib/html/index.js"
  }
}
