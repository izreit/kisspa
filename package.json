{
  "name": "ficco",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "types": "lib/index.d.js",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run _build:parser && npm run _build:ts && npm run _build:dist",
    "test": "vitest",
    "cov": "vitest run --coverage",
    "_build:parser": "tspeg --enable-memo ./src/upwind/parser.peg ./src/upwind/parser.ts",
